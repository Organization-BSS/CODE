#import "AppWindowFactory.h"
@import UIKit;

@implementation AppWindowFactory

// 返回app的window
+ (UIWindow *)appRootWindow {
    
    UIWindow *window = [[UIWindow alloc]initWithFrame:[[UIScreen mainScreen] bounds]];
    return window;
}
@end
